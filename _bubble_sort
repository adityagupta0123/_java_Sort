package Arrays;

public class Bubblesort {
    public static void main(String[]args){

        int a[]={1,-2,-33,7,31,12};
        int n=a.length;

        for (int i=0; i<n-1; i++ ){
            for(int j=0; j<n-1-i; j++){

                if(a[j+1]<a[j]){
                    int temp = a[j+1];
                     a[j+1]= a[j];
                     a[j]= temp;

                }
            }
        }
        for(int item:a){
            System.out.print(item+" ");
        }
    }
}
